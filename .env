# VoIP Frontend Configuration

# Backend API URL (use localhost since backend and frontend are on same machine)
REACT_APP_API_URL=http://localhost:8080

# Asterisk SIP Server Configuration (your Asterisk server)
REACT_APP_SIP_SERVER=172.20.10.14
REACT_APP_SIP_PORT=8088
REACT_APP_SIP_WS_URL=ws://172.20.10.14:8088/ws

# WebSocket URL for signaling (use localhost since backend and frontend are on same machine)
REACT_APP_WS_URL=ws://localhost:8080/ws

# Client IP (use your PC's IP)
REACT_APP_CLIENT_IP=172.20.10.8

# Debug mode
REACT_APP_DEBUG=true
